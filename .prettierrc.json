{
  "semi": true,
  "arrowParens": "always",
  "singleQuote": true,
  "printWidth": 100,
  "bracketSpacing": true,
  "trailingComma": "none",
  "useTabs": false,
  "tabWidth": 2,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": true,
  "importOrder": [
    "^components/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ]
}